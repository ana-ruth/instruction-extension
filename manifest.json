{
  "manifest_version": 3,
  "name": "InstructionGiver",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": ["<all_urls>", "https://api.gemini.google.com/*"],
  "action": {
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "service-worker.js"
  }
}

